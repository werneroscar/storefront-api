{
  "name": "storefront_backend",
  "version": "0.1.0",
  "description": "",
  "main": "server.ts",
  "scripts": {
    "start": "nodemon src/server.ts",
    "watch": "tsc-watch --esModuleInterop src/server.ts --outDir ./dist --onSuccess \"node ./dist/server.js\"",
    "tsc": "tsc",
    "jasmine": "jasmine",
    "build": "npx tsc",
    "test": "set ENV=test && npx tsc && db-migrate --env test reset && db-migrate --env test up && jasmine",
    "lint": "npx eslint . --ext .ts",
    "prettier": "prettier --config .prettierrc src/**/*.ts --write",
    "initdb": "db-migrate up"
  },
  "author": "Udacity",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.1.0",
    "body-parser": "^1.20.1",
    "cors": "^2.8.5",
